{
  "name": "cyberops-backend",
  "version": "1.0.0",
  "description": "Backend server for CyberOps Hacker Desktop Dashboard",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:rollback": "sequelize-cli db:migrate:undo",
    "migrate:create": "sequelize-cli migration:generate --name"
  },
  "keywords": [
    "dashboard",
    "monitoring",
    "security"
  ],
  "author": "CyberOps Team",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "ws": "^8.16.0",
    "socket.io": "^4.6.1",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.17.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "joi": "^17.12.1",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "redis": "^4.6.12",
    "dotenv": "^16.4.1",
    "uuid": "^9.0.1",
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/ws": "^8.5.10",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
